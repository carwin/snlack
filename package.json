{
  "name": "Snyk for Slack (unofficial)",
  "version": "1.0.0",
  "description": "A Slack app for communicating with Snyk",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc -p .",
    "build:watch": "tsc -w -p .",
    "clean": "rimraf ./dist",
    "format": "prettier --write 'src/**/*.ts'",
    "ts:exports:glob": "./scripts/exportGlob.sh actions controllers exceptions middleware utils views types",
    "start": "npm run build && node dist/app.js",
    "start2": "npm run build && node dist/app2.js"
  },
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^3.3.0",
    "@slack/events-api": "^3.0.1",
    "@snyk/passport-snyk-oauth2": "^1.0.3",
    "axios": "^0.21.4",
    "config": "^3.3.7",
    "cryptr": "^6.0.2",
    "dotenv": "^8.2.0",
    "envar-check": "0.0.3",
    "express-rate-limit": "^6.5.1",
    "express-session": "^1.17.3",
    "jwt-decode": "^3.1.2",
    "lowdb": "github:dankreiger/lowdb#chore/esm-cjs-hybrid-WITH-LIB",
    "luxon": "^3.0.1",
    "nodemon": "^2.0.12",
    "passport": "^0.4.1",
    "passport-oauth2": "^1.6.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@types/config": "0.0.39",
    "@types/cryptr": "^4.0.1",
    "@types/express-session": "^1.17.5",
    "@types/lowdb": "^1.0.11",
    "@types/luxon": "^3.0.0",
    "@types/node": "^14.14.35",
    "@types/passport": "^1.0.7",
    "@types/passport-oauth2": "^1.4.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "typescript-eslint-language-service": "^5.0.0",
    "uuid": "^8.3.2"
  },
  "nodemonConfig": {
    "ignore": [
      "db/*"
    ]
  }
}
